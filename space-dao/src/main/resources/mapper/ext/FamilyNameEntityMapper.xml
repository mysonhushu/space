<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huyouxiao.space.dao.mapper.FamilyNameEntityMapper">
    <select id="selectByFamilyName" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from family_name
        where family_name = #{familyName,jdbcType=VARCHAR}
    </select>
</mapper>